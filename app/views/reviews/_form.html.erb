<%= render partial: "layouts/error", locals: {object: @review} %>

<%= form_for ([@hike, @review]) do |f| %>
    <%= f.text_area :content %><br>
    <%= f.hidden_field :hike_id, value: @review.hike.id %><br>
    <%= f.hidden_field :user_id, value: current_user.id %><br>
    <%= f.submit %>
    <% end %>