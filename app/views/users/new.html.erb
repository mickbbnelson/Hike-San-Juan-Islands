<h1> Sign Up </h1>

<%= render partial: "layouts/error", locals: {object: @user} %>

<%= form_for(@user, url:sign_up_path) do |f| %>
    <h5>Name: <%= f.text_field :name %></h5>
    <h5>Email: <%= f.text_field :email_address %></h5>
    <h5>Password: <%= f.password_field :password %></h5>
    <%= f.submit "Submit" %>
  <% end %> 